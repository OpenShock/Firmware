		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.9",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^6.0.0",
		"vitest": "^2.0.4"
	},
	"engines": {
		"node": "^20.18",
		"pnpm": "^9"
	},
	"volta": {
		"node": "20.18.0"
	}
