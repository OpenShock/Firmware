		"svelte-check": "^4.3.4",
		"svelte-sonner": "^1.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.7",
		"vitest": "^4.0.15"
	},
	"engines": {
		"node": "^22.14.0",
		"pnpm": "^10.6.4"
	},
	"volta": {
		"node": "22.14.0"
	},
